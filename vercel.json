{
  "functions": {
    "app/api/clients/import-multi-sheet/route.ts": {
      "maxDuration": 60
    },
    "app/api/clients/import-chunked/route.ts": {
      "maxDuration": 30
    },
    "app/api/clients/import-large-excel/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/clients/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/gym-coach-platform/$1"
    }
  ]
}